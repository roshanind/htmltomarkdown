{
  "env": {
    "browser": true,
    "es2021": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "plugin:@cspell/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "dot-notation": 0,
    "prettier/prettier": "error",
    "@cspell/spellchecker": [
      "error",
      {
        "customWordListFile": ".dictionaryrc"
      }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}